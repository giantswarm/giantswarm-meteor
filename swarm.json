{
    "app_name": "meteor-test",
    "services": [
        {
            "service_name": "meteor-test",
            "components": [
                {
                    "component_name": "meteor-test",
                    "image": "ulexus/meteor",
                    "ports": [ 80 ],
                    "domains": { "meteor-puja.gigantic.io": 80 },
                    "env":
                    {
            			"REPO": "https://github.com/Differential/meteor-boilerplate",
            			"ROOT_URL": "http://meteor-puja.gigantic.io"
          			},
                    "dependencies": [
                        {
                            "name": "mongo",
                            "port": 27017
                        }
                    ]
                },
                {
                    "component_name": "mongo",
                    "image": "mongo",
                    "ports": [27017]
                }
            ]
        }
    ]
}